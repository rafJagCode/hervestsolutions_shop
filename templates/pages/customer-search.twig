<table id="example" class="table table-striped table-bordered" style="width:100%">
	<thead>
		<tr>
			<th>Imie</th>
			<th>Nazwisko</th>
			<th>Adres</th>
			<th>Telefon</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>
		{% for user in users %}
			<tr>
				<td>{{user.firstName}}</td>
				<td>{{user.lastName}}</td>
				<td>{{user.address}}</td>
				<td>{{user.telephone}}</td>
				<td>{{user.email}}</td>
			</tr>
		{% endfor %}
	</tbody>
	<tfoot>
		<tr>
			<th>Imie</th>
			<th>Nazwisko</th>
			<th>Adres</th>
			<th>Telefon</th>
			<th>Email</th>
		</tr>
	</tfoot>
</table>
